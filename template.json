{
	"package": {
		"dependencies": {
			"@emotion/react": "^11.7.1",
			"@emotion/styled": "^11.6.0",
			"@mui/material": "^5.2.6"
		},
		"devDependencies": {
			"@types/chrome": "^0.0.174",
			"concurrently": "^6.5.1",
			"nodemon": "^2.0.15"
		},
		"scripts": {
			"update": "find ./public/* ! -name index.html -maxdepth 0 -print0 | xargs -0 -I file cp -vr file ./extension",
			"dev": "concurrently --raw \"npm:start\" \"nodemon\""
		},
		"browserslist": {
			"production": [
				"Chrome > 4 and > 1%"
			],
			"development": [
				"last 2 Chrome versions"
			]
		},
		"nodemonConfig": {
			"exec": "npm run update",
			"watch": [
				"public"
			],
			"delay": 10000
		}
	}
}